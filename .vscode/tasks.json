{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Clean Gabriel",
      "type": "shell",
      "command": "make clean",
      "options": {
        "cwd": "${workspaceFolder}/golden/GabrielGraph"
      },
      "problemMatcher": []
    },
    {
      "label": "Debug Gabriel",
      "type": "shell",
      "command": "make debug",
      "options": {
        "cwd": "${workspaceFolder}/golden/GabrielGraph"
      },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "Build Gabriel",
      "dependsOn": [
        "Clean Gabriel",
        "Debug Gabriel"
      ],
      "dependsOrder": "sequence",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Clean Filter",
      "type": "shell",
      "command": "make clean",
      "options": {
        "cwd": "${workspaceFolder}/golden/LowDegreeVerticesFilter"
      },
      "problemMatcher": []
    },
    {
      "label": "Debug Filter",
      "type": "shell",
      "command": "make debug",
      "options": {
        "cwd": "${workspaceFolder}/golden/LowDegreeVerticesFilter"
      },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "Build Filter",
      "dependsOn": [
        "Clean Filter",
        "Debug Filter"
      ],
      "dependsOrder": "sequence",
      "group": {
        "kind": "build"
      }
    },
    {
      "label": "Build All",
      "dependsOn": [
        "Build Gabriel",
        "Build Filter"
      ],
      "dependsOrder": "sequence",
      "group": {
        "kind": "build"
      }
    }
  ]
}
