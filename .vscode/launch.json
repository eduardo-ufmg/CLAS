{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Gabriel Graph - synthetic",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/dev/GabrielGraph/gabrielGraph",
      "args": [
        "${workspaceFolder}/data/synthetic/synthetic.csv",
      ],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}/dev/GabrielGraph",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build Gabriel"
    },
    {
      "name": "Debug Filter - synthetic",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/dev/Filter/filter",
      "args": [
        "${workspaceFolder}/dev/GabrielGraph/output/synthetic.csv",
      ],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}/dev/Filter",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build Filter"
    },
    {
      "name": "Debug Hyperplane - synthetic",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/dev/classifiers/Common/hyperplanes",
      "args": [
        "${workspaceFolder}/dev/GabrielGraph/output/synthetic-filtered.csv",
      ],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}/dev/classifiers/Common",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build Hyperplane"
    },
    {
      "name": "Debug Chip - synthetic",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/dev/classifiers/chip-clas/chip-clas",
      "args": [
        "${workspaceFolder}/dev/GabrielGraph/output/synthetic-filtered.csv",
        "${workspaceFolder}/dev/classifiers/Common/output/synthetic-filtered.csv",
        "${workspaceFolder}/data/synthetic/to-classify.csv",
      ],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}/dev/classifiers/chip-clas",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build Chip"
    }
  ]
}