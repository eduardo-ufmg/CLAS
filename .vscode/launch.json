{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Gabriel Graph - iris",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/dev/GabrielGraph/gabrielGraph",
      "args": [
        "${workspaceFolder}/data/iris/iris.csv",
      ],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
